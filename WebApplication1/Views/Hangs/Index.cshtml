@model IEnumerable<WebApplication1.Models.Hang>

@{
    ViewData["Title"] = "NhaSanXuatAD";
    Layout = "~/Views/Shared/_LayoutAD.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        <a asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Mahang)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Tenhang)
                </th>
                
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Mahang)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tenhang)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Mahang">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Mahang">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Mahang">Delete</a>
                </td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>
